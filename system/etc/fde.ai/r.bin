#!/system/bin/sh
### FDE.AI | FeraVolt. 2019 ###
while true;do BOOT=$(getprop sys.boot_completed);if [ "$BOOT" -eq "1" ];then sleep 18;break;else sleep 9;fi;done;if [ -d /sbin/.magisk/img/FDE ];then SYSFD=/sbin/.magisk/img/FDE/system;else SYSFD=/system;fi;killall -q fde>/dev/null 2>&1;rm -Rf /dev/fdeai>/dev/null 2>&1;mkdir /dev/fdeai;chmod -R 755 /dev/fdeai;A=$(getprop ro.product.cpu.abi);if [ "$A" = "$(echo "$A"|grep "arm64")" ];then cp -f $SYSFD/etc/fde.ai/fde64 /dev/fdeai/fde;rm -Rf $SYSFD/etc/fde.ai/fde32;rm -Rf $SYSFD/etc/fde.ai/fde86;elif [ "$A" = "$(echo "$A"|grep "armeabi")" ];then cp -f $SYSFD/etc/fde.ai/fde32 /dev/fdeai/fde;rm -Rf $SYSFD/etc/fde.ai/fde64;rm -Rf $SYSFD/etc/fde.ai/fde86;elif [ "$A" = "$(echo "$A"|grep "x86_64")" ];then cp -f $SYSFD/etc/fde.ai/fde86 /dev/fdeai/fde;rm -Rf $SYSFD/etc/fde.ai/fde64;rm -Rf $SYSFD/etc/fde.ai/fde32;else cp -f $SYSFD/etc/fde.ai/fde32 /dev/fdeai/fde;rm -Rf $SYSFD/etc/fde.ai/fde64;rm -Rf $SYSFD/etc/fde.ai/fde86;fi;chmod 755 /dev/fdeai/fde;/dev/fdeai/fde>/dev/null 2>&1 &
sleep 450;echo "FDE" > /sys/power/wake_unlock;
